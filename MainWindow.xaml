        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="1200" ResizeMode="NoResize" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="0" BorderThickness="0">
    <Grid x:Name="CommonGrid" Width="1200" Height="600" ClipToBounds="True">
        <Label x:Name="EndpointLabel" Content="Endpoint:" HorizontalAlignment="Left" Margin="25,0,0,0" VerticalAlignment="Top" Height="26" Width="60" Grid.Row="1"/>
        <ComboBox x:Name="MethodName" HorizontalAlignment="Left" Margin="100,0,0,0" VerticalAlignment="Top" Width="120" Height="22" Grid.Row="1"/>


        <TextBox x:Name="Endpoint" HorizontalAlignment="Left" Height="23" Margin="220,65,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" Grid.Row="1"/>








    </Grid>
</Window>
